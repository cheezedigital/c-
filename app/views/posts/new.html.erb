<h1>New Post</h1>

<h2><%= form_for @post do |f| %></h2>
<h2><%= f.label :title%></h2>
<h2><%= f.text_field :title%></h2>

<h2><%= f.label :author %></h2>
<h2><%= f.text_field :author %></h2>

<h2><%= f.label :description %></h2>
<h2><%= f.text_field :description %></h2>

<h2><%= f.submit %></h2>
<% end %>
